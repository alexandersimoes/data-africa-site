  "name": "data-africa-site",
  "version": "1.0.0",
  "description": "Front-end for Data Africa",
  "main": "index.js",
  "scripts": {
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,public",
    "babel-node": "babel-node --presets es2015,stage-0 --ignore node_modules,public",
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon",
    "build:dev": "NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "NODE_ENV=production HOSTNAME=dataafrica.io webpack --colors --config ./webpack/webpack.config.prod.js",
    "build:node": "NODE_ENV=production HOSTNAME=dataafrica.io npm run babel -- server.js -o ./index.js",
    "build": "npm run build:prod && npm run build:node"
  },
